  "scripts": {
    "start": "dotenv -e .env craco start",
    "build": "dotenv -e .env craco build",
    "build:dev": "dotenv -e .env craco build",
    "test": "dotenv -e .env craco test",
    "eject": "craco eject",
    "test:ci": "CI=true craco test",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --max-warnings=0 './src/**/*.{ts,tsx}'",
    "lint:prettier": "prettier --check './src/**/*.{ts,tsx}'",
    "lint-fix": "run-p lint-fix:*",
    "lint-fix:eslint": "eslint --fix './src/**/*.{ts,tsx}'",
    "lint-fix:prettier": "prettier --write './src/**/*.{ts,tsx}'",
    "prepare": "cd .. && husky install app/.husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "11.2.0",
